{
  "manifest_version": 3,
  "name": "WSDL Scanner",
  "version": "1.1",
  "permissions": [
    "webRequest",
    "storage",
	"notifications",
	"background"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
